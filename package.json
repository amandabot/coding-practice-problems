  "scripts": {
    "tsc": "npm run cleanDir && tsc -w",
    "cleanDir": "if exist tmp rd /s /q tmp",
    "all": "protractor tmp/protractor.config.js",
    "debug": "node --inspect-brk ./node_modules/.bin/protractor asyncAwait/protractor.config.js",
