    "full": "npm run all && npm run direct && npm run process"
  },
  "dependencies": {
    "banking": "1.2.0",
    "jasmine": "3.6.1",
    "moment": "2.27.0",
    "protractor": "7.0.0",
    "typescript": "3.9.7"
  },
  "devDependencies": {
    "@types/jasmine": "3.5.13",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.6.0",
    "ts-node": "9.0.0",
    "tslint": "6.1.3"
  }
